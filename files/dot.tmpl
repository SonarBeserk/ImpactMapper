// Generated by impact mapper, DO NOT EDIT
digraph {{.Name}} {
	{{if eq .Mode "horizontal"}}newrank=true;{{end}}
	node [style="filled,rounded", shape=box];

	subgraph cluster_goal {
		node[color=darkgoldenrod]
		{{- range $key, $value := .Goals}}
		{ Goal{{$key}}[label="{{$value.Name}}"] }
		{{- end}}
		label = "Goal";
		color = darkgoldenrod
	}

	subgraph cluster_actor {
		node [color=forestgreen]
		{{- range $key, $value := .Actors}}
		Goal{{$value.Goal}} -> { Actor{{$key}}[label="{{$value.Name}}"] }
		{{- end}}
		label = "Actor";
		color = forestgreen
	}

	subgraph cluster_impact {
		node[color=dodgerblue]
		{{- range $key, $value := .Impacts}}
		Actor{{$value.Actor}} -> { Impact{{$key}}[label="{{$value.Name}}"] }
		{{- end}}
		label = "Impact";
		color = dodgerblue
	}

	subgraph cluster_deliverable {
		node[color=deeppink]
		{{- range $key, $value := .Deliverables}}
		Impact{{$value.Impact}} -> { Deliverable{{$key}}[label="{{$value.Name}}"] }
		{{- end}}
		label = "Deliverable";
		color = deeppink
	}

	{{if eq .Mode "horizontal"}}{ rank=same; Goal1; Actor1; Impact1; Deliverable1; }{{end}}
}